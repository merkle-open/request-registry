{
  "name": "request-registry",
  "version": "0.6.0",
  "license": "MIT",
  "description": "Type safe promise based HTTP request registry for the browser",
  "bugs": "https://github.com/namics/request-registry/issues",
  "homepage": "https://github.com/namics/request-registry",
  "repository": "https://github.com/namics/request-registry.git",
  "author": "Jan Nicklas",
  "main": "dist/index.js",
  "umd:main": "dist/request-registry.umd.production.js",
  "module": "dist/request-registry.es.production.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdx build",
    "commit": "git-cz",
    "prettier": "prettier --write \"**/*.*(js|jsx|ts|tsx|json)\"",
    "prepublishOnly": "pkg-ok",
    "release": "standard-version",
    "size": "npx size-limit",
    "start": "tsdx watch",
    "test": "tsdx test"
  },
  "devDependencies": {
    "@types/fetch-mock": "7.3.0",
    "@types/jest": "24.0.13",
    "fetch-mock": "7.3.3",
    "node-fetch": "2.6.0",
    "pkg-ok": "2.3.1",
    "tsdx": "0.6.1",
    "typescript": "3.5.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "size-limit": [
    {
      "path": "dist/request-registry.cjs.production.js",
      "limit": "1.5 KB"
    }
  ]
}
